local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Loper X Hub", "GrapeTheme")

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Farm")

Section:NewToggle("Farm Level", "ToggleInfo", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Lopernaaa/LoperHub/main/Autofarm")
    else
        print("off autofarm")
    end
end)
